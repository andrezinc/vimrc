set nocompatible
filetype off
syntax on
filetype on
colorschem industry
filetype plugin on
autocmd filetype python nnoremap <F9> :w <bar> exec '!python '.shellescape('%')<CR>
autocmd filetype c nnoremap <F9> :w <bar> exec '!gcc '.shellescape('%').' -o '.shellescape('%:r').' -lm && ./'.shellescape('%:r')<CR>
autocmd filetype c nnoremap <F7> :w <bar> exec '!makewiz'<CR>:make<CR>:!./main
autocmd filetype cpp nnoremap <F7> :w <bar> exec '!makewiz'<CR>:make<CR>:!./main
autocmd filetype cpp nnoremap <F9> :w <bar> exec '!g++ '.shellescape('%').' -o '.shellescape('%:r').' && ./'.shellescape('%:r')<CR>
set relativenumber
set cursorline 
set cursorcolumn
set tabstop=4
call plug#begin()
Plug 'epheien/termdbg'
call plug#end()
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
Plugin 'jiangmiao/auto-pairs'
Plugin 'Valloric/YouCompleteMe'
call vundle#end()            " required
filetype plugin indent on    " required
set encoding=utf-8 
